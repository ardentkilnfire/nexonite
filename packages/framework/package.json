{
    "name": "nexonite",
    "version": "0.1.0",
    "author": "Ardentkilnfire <akt01.social@gmail.com>",
    "description": "A framework for building discord bot using discord.js",
    "license": "MIT",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "LICENSE",
        "README.md",
        "CHANGELOG.md"
    ],
    "scripts": {
        "dev": "tsup --watch src",
        "build": "tsup",
        "lint": "tsc",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "@types/node": "^20.12.12",
        "discord.js": "^14.15.2",
        "prettier": "^3.2.5",
        "tsup": "^8.0.2",
        "typescript": "^5.4.5"
    },
    "peerDependencies": {
        "discord.js": "^14.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ardentkilnfire/nexonite.git"
    },
    "bugs": {
        "url": "https://github.com/ardentkilnfire/nexonite/issues"
    },
    "homepage": "https://github.com/ardentkilnfire/nexonite",
    "engines": {
        "node": ">= 18"
    },
    "keywords": [
        "nexonite",
        "discord.js framework"
    ],
    "dependencies": {
        "zod": "^3.23.8"
    }
}
